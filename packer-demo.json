{
  "builders": [
    {
      "type": "googlecompute",
      "project_id": "${{project_id}}",
      "account_file": "creden.json",
      "source_image_family": "ubuntu-2004-lts",
      "image_name": "my-golden-image",
      "image_family": "my-golden-family",
      "zone": "us-central1-a",
      "ssh_username": "packer",
      "image_description": "My custom image created with Packer",
      "image_labels": {
        "environment": "production",
        "created-by": "packer"
      }
    }
  ],

  "provisioners": [
    {
      "type": "shell",
      "inline": [
        "sudo apt-get update",
        "sudo apt-get install -y nginx"
      ]
    }
    // Add additional provisioners as needed
  ],

  "variables": {
    "project_id": "playground-s-11-9efb5b94"
    // Add other variables as needed
  }
}
